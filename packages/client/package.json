{
  "name": "@learn-languages/client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "build:ci": "tsc && pnpm run build",
    "build:debug": "vite build --debug",
    "build:dev": "vite build --mode development",
    "depcheck": "pnpm dlx depcheck",
    "lint": "eslint 'src/**/*.{tsx,ts}' --quiet --max-warnings 1",
    "lint:fix": "eslint 'src/**/*.{tsx,ts}' --fix ",
    "lint:warn": "eslint 'src/**/*.{tsx,ts}'",
    "start": "vite",
    "start:debug": "vite --debug",
    "test": "echo \"TODO: vitest run\"",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch",
    "update": "pnpm dlx npm-check -u -E -s"
  },
  "dependencies": {
    "@ant-design/icons": "5.2.6",
    "@apollo/client":"3.8.10",
    "@fontsource/roboto": "5.0.8",
    "@learn-languages/common": "workspace:^",
    "antd": "^5.13.2",
    "ky": "^1.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.21.3",
    "yup": "1.3.3",
    "zustand": "4.5.0"
  },
  "devDependencies": {
    "@learn-languages/eslint-common": "workspace:^",
    "@tanstack/eslint-plugin-query": "5.20.1",
    "@testing-library/jest-dom": "6.2.1",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.2",
    "@types/react-dom": "18.2.18",
    "@vitejs/plugin-basic-ssl": "1.1.0",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/ui": "1.2.1",
    "autoprefixer": "10.4.17",
    "jsdom": "23.0.1",
    "postcss": "8.4.33",
    "sass": "1.70.0",
    "typescript": "5.3.3",
    "vitest": "1.2.1"
  }
}
